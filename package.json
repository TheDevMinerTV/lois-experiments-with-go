{
  "name": "go",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:css\" \"npm run dev:go\"",
    "dev:css": "tailwindcss -i tailwind.css -o style.css --watch",
    "dev:go": "nodemon --signal SIGTERM --watch '*/**' --ext go,css,templ --exec \"templ generate && go run main.go\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  }
}
